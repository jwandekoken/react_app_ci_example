{
  "branches": ["main"],
  "repositoryUrl": "https://github.com/jwandekoken/GA_react-app-ci-example",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "build.zip",
            "label": "Build"
          },
          {
            "path": "coverage.zip",
            "label": "Coverage"
          }
        ]
      }
    ]
  ]
}
